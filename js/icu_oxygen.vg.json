{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400, "height": 300,
  "data": {"url": "https://raw.githubusercontent.com/theonlyanish/FIT3179-Assig2/main/data/oxygen_shortage_deaths_in_india.csv"},
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "month", "field": "date_of_incident", "type": "temporal",
      "axis": {"format": "%d/%b"}
    },
    "y": {
      "aggregate": "median", "field": "no_of_deaths", "type": "quantitative"
    },
    "color": {
      "field": "state",
      "scale": {"scheme": "category20b"}
    }
  }
}
